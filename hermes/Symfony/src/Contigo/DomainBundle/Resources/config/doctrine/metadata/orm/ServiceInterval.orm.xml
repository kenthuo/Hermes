<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ServiceInterval" table="service_interval">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="subscriberId" type="bigint" column="subscriber_id"/>
    <field name="organizationId" type="bigint" column="organization_id"/>
    <field name="unit" type="integer" column="unit"/>
    <field name="unitOfMeasurement" type="string" column="unit_of_measurement" length="10"/>
    <field name="maintenanceId" type="bigint" column="maintenance_id"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
