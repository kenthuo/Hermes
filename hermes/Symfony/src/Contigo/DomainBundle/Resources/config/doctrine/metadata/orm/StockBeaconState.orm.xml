<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StockBeaconState" table="stock_beacon_state">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="stockId" type="bigint" column="stock_id"/>
    <field name="beaconStateId" type="bigint" column="beacon_state_id"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="iscurrent" type="boolean" column="iscurrent"/>
    <field name="accessListId" type="bigint" column="access_list_id"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
