<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventAuxalarmTt" table="event_auxalarm_tt">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="beaconId" type="bigint" column="beacon_id"/>
    <field name="locateId" type="bigint" column="locate_id"/>
    <field name="networkTimestamp" type="bigint" column="network_timestamp"/>
    <field name="lon" type="decimal" column="lon"/>
    <field name="lat" type="decimal" column="lat"/>
    <field name="engineHours" type="bigint" column="engine_hours"/>
    <field name="lpa" type="string" column="lpa"/>
    <field name="mpa" type="string" column="mpa"/>
    <field name="hpa" type="string" column="hpa"/>
    <field name="batteryLevel" type="integer" column="battery_level"/>
    <field name="inputName" type="string" column="input_name" length="100"/>
    <field name="senId" type="bigint" column="sen_id"/>
    <field name="scenarioName" type="string" column="scenario_name" length="100"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
