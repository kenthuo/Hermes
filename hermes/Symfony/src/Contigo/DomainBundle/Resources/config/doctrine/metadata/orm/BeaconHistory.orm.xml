<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BeaconHistory" table="beacon_history">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="stockId" type="bigint" column="stock_id"/>
    <field name="assetId" type="bigint" column="asset_id"/>
    <field name="planId" type="bigint" column="plan_id"/>
    <field name="registeredOrgId" type="bigint" column="registered_org_id"/>
    <field name="dependentId" type="bigint" column="dependent_id"/>
    <field name="beaconmodelId" type="bigint" column="beaconmodel_id"/>
    <field name="registeredSubscriberId" type="bigint" column="registered_subscriber_id"/>
    <field name="ownerCustomerId" type="bigint" column="owner_customer_id"/>
    <field name="name" type="string" column="name" length="40"/>
    <field name="iconId" type="bigint" column="icon_id"/>
    <field name="xid" type="bigint" column="xid"/>
    <field name="isDeleted" type="boolean" column="is_deleted"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="accessListId" type="bigint" column="access_list_id"/>
    <field name="canNotifyStation" type="boolean" column="can_notify_station"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
