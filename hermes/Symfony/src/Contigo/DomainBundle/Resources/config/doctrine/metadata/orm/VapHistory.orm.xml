<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VapHistory" table="vap_history">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="assetId" type="bigint" column="asset_id"/>
    <field name="dependentId" type="bigint" column="dependent_id"/>
    <field name="itemName" type="string" column="item_name" length="150"/>
    <field name="beaconId" type="bigint" column="beacon_id"/>
    <field name="beaconName" type="string" column="beacon_name" length="150"/>
    <field name="isMultiple" type="boolean" column="is_multiple"/>
    <field name="xid" type="bigint" column="xid"/>
    <field name="isDeleted" type="boolean" column="is_deleted"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="accessListId" type="bigint" column="access_list_id"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
