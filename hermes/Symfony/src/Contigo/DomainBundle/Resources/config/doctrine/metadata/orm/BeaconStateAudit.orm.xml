<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BeaconStateAudit" table="beacon_state_audit">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="beaconId" type="bigint" column="beacon_id"/>
    <field name="subId" type="bigint" column="sub_id"/>
    <field name="orgId" type="bigint" column="org_id"/>
    <field name="planId" type="bigint" column="plan_id"/>
    <field name="assetId" type="bigint" column="asset_id"/>
    <field name="dependentId" type="bigint" column="dependent_id"/>
    <field name="opUserId" type="bigint" column="op_user_id"/>
    <field name="customerId" type="bigint" column="customer_id"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="action" type="string" column="action" length="255"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
