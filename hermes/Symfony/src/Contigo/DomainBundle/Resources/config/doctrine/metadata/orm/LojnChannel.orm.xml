<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LojnChannel" table="lojn_channel">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="name" type="text" column="name"/>
    <field name="description" type="text" column="description"/>
    <field name="isDefault" type="boolean" column="is_default"/>
    <field name="createdTs" type="bigint" column="created_ts"/>
    <field name="createdBy" type="bigint" column="created_by"/>
    <field name="updatedTs" type="bigint" column="updated_ts"/>
    <field name="updatedBy" type="bigint" column="updated_by"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
