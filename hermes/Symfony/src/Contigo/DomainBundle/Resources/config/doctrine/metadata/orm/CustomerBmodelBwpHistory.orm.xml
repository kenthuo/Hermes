<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CustomerBmodelBwpHistory" table="customer_bmodel_bwp_history">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="customerId" type="bigint" column="customer_id"/>
    <field name="beaconmodelId" type="bigint" column="beaconmodel_id"/>
    <field name="bwpPlanId" type="bigint" column="bwp_plan_id"/>
    <field name="xid" type="bigint" column="xid"/>
    <field name="isDeleted" type="boolean" column="is_deleted"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="accessListId" type="bigint" column="access_list_id"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
