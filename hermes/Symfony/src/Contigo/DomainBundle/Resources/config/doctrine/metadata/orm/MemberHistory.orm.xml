<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MemberHistory" table="member_history">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="customerId" type="bigint" column="customer_id"/>
    <field name="daylightSaving" type="boolean" column="daylight_saving"/>
    <field name="username" type="string" column="username" length="20"/>
    <field name="password" type="string" column="password" length="200"/>
    <field name="type" type="string" column="type" length="20"/>
    <field name="firstname" type="string" column="firstname" length="255"/>
    <field name="lastname" type="string" column="lastname" length="255"/>
    <field name="employeeId" type="string" column="employee_id" length="255"/>
    <field name="phAreacode" type="string" column="ph_areacode" length="4"/>
    <field name="phNumber" type="string" column="ph_number" length="20"/>
    <field name="phExtension" type="string" column="ph_extension" length="4"/>
    <field name="dateDisplayFormat" type="string" column="date_display_format" length="20"/>
    <field name="xid" type="bigint" column="xid"/>
    <field name="isDeleted" type="boolean" column="is_deleted"/>
    <field name="timestamp" type="bigint" column="timestamp"/>
    <field name="accessListId" type="bigint" column="access_list_id"/>
    <field name="timezone" type="float" column="timezone"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
