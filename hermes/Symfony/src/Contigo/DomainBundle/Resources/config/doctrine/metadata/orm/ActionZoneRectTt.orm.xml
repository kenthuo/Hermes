<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActionZoneRectTt" table="action_zone_rect_tt">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="subscriberId" type="bigint" column="subscriber_id"/>
    <field name="organizationId" type="bigint" column="organization_id"/>
    <field name="lat1" type="decimal" column="lat1"/>
    <field name="lon1" type="decimal" column="lon1"/>
    <field name="lat2" type="decimal" column="lat2"/>
    <field name="lon2" type="decimal" column="lon2"/>
    <field name="boundary" type="string" column="boundary" length="20"/>
    <field name="schedule" type="string" column="schedule" length="1"/>
    <field name="scheduleId" type="bigint" column="schedule_id"/>
    <field name="name" type="string" column="name" length="100"/>
    <field name="description" type="text" column="description"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
