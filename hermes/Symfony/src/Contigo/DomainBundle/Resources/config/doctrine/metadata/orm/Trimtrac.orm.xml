<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Trimtrac" table="trimtrac">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="beaconId" type="bigint" column="beacon_id"/>
    <field name="issuedCmd" type="string" column="issued_cmd"/>
    <field name="issuedCmdTimestamp" type="bigint" column="issued_cmd_timestamp"/>
    <field name="lastTrackingStopped" type="bigint" column="last_tracking_stopped"/>
    <field name="securityPassword" type="string" column="security_password"/>
    <field name="hpaEnable" type="boolean" column="hpa_enable"/>
    <field name="mpaEnable" type="boolean" column="mpa_enable"/>
    <field name="commMode" type="string" column="comm_mode"/>
    <field name="trackingFreq" type="integer" column="tracking_freq"/>
    <field name="motionFreq" type="integer" column="motion_freq"/>
    <field name="nonMotionFreq" type="integer" column="non_motion_freq"/>
    <field name="scheduledMode" type="string" column="scheduled_mode"/>
    <field name="scheduledTime" type="bigint" column="scheduled_time"/>
    <field name="sequence" type="integer" column="sequence"/>
    <field name="version" type="string" column="version"/>
    <field name="unitId" type="string" column="unit_id"/>
    <field name="prevZoneStatus" type="bigint" column="prev_zone_status"/>
    <field name="seqNumber" type="integer" column="seq_number"/>
    <field name="lockdown" type="boolean" column="lockdown"/>
    <field name="motionSensitivity" type="string" column="motion_sensitivity"/>
    <field name="notificationType" type="integer" column="notification_type"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
