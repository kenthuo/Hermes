<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ScenarioLevelNotificationHistory" table="scenario_level_notification_history">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="scenarioId" type="bigint" column="scenario_id"/>
    <field name="actionIndexId" type="bigint" column="action_index_id"/>
    <field name="actionId" type="bigint" column="action_id"/>
    <field name="introTs" type="bigint" column="intro_ts"/>
    <field name="retireTs" type="bigint" column="retire_ts"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
